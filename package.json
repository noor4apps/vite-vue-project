{
  "name": "vite-vue-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode mock",
    "build": "vue-tsc && vite build --mode production",
    "build-mock": "vue-tsc --noEmit && vite build --mode mock",
    "preview": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "i18next": "^23.11.3",
    "vue": "^3.4.21",
    "vue-i18n": "^9.13.1"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.3",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "axios": "^1.6.8",
    "jsdom": "^24.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.5.3",
    "vue-tsc": "^2.0.6"
  }
}
